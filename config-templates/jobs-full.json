{
    "workdir":"/home/gisadmin/Sicherungen/Snapshots/$today$",
    "jobs": [
        {
            "name":"borg"
            ,"command":["borg-create.sh"]
            ,"next-job":"borg-prune"
        },
        {
            "name":"borg-prune"
            ,"command":["borg-prune.sh"]
            ,"next-job":"postgresql"
        },
        {
            "name":"postgresql-neu"
            ,"command":["pgbackrest.sh", "kvwmap_prod_pgsql", "full"]
            ,"next-job":"mariadb"
        },
        {
            "name":"postgresql-alt"
            ,"command":["dump-postgresql.sh", "/root/.config/backup/pgdump.kvwmap_prod.conf"]
            ,"next-job":"mariadb"
        },
        {
            "name":"mariadb"
            ,"command":["dump-mariadb.sh", "/etc/backup/mariadb.kvwmap_prod.conf"]
        }
    ]
}
